  "bugs": {
    "url": "https://github.com/JJG1488/google-books-search/issues"
  },
  "homepage": "https://github.com/JJG1488/google-books-search#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "concurrently": "^6.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.12.7",
    "node-notifier": "^8.0.2",
    "react": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3"
  },
  "description": ""
}
